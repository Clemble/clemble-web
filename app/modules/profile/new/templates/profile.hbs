<div class="container">
	<h4>Profile</h4>
	<form class="form-horizontal" role="form">
		<div class="form-group">
			<label for="country" class="control-label">Country</label>
			<div class="col-sm-10">
				<input id="country" type="text" class="form-control" placeholder="Country">
			</div>
		</div>
		<div class="form-group">
			<label for="nickName" class="control-label">Nick</label>
			<div class="col-sm-10">
				<input id="nickName" type="text" class="form-control" placeholder="Nick Name">
			</div>
		</div>
		<div class="form-group">
			<label for="firstName" class="control-label">First Name</label>
			<div class="col-sm-10">
				<input id="firstName" type="text" class="form-control" placeholder="First Name">
			</div>
		</div>
		<div class="form-group">
			<label for="lastName" class="control-label">Last Name</label>
			<div class="col-sm-10">
				<input id="lastName" type="text" class="form-control" placeholder="Last Name">
			</div>
		</div>
		<div class="form-group">
			<label for="gender" class="control-label">Gender</label>
			<div class="col-sm-10">
				<input id="gender" type="text" class="form-control" placeholder="Gender">
			</div>
		</div>
		<div class="form-group">
			<label for="birthDate" class="control-label">Birth Date</label>
			<div class="col-sm-10">
				<input id="birthDate" type="text" class="form-control" placeholder="Birth Date">
			</div>
		</div>
		<div class="form-group">
			<label for="imageUrl" class="control-label">Image</label>
			<div class="col-sm-10">
				<input id="imageUrl" type="text" class="form-control" placeholder="Image">
			</div>
		</div>
	</form>
</div>
