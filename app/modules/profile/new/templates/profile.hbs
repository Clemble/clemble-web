<div class="container">
	<h4>Profile</h4>
	<form class="form-horizontal" role="form">
		<div class="form-group">
			<label for="country" class="control-label">Country</label>
			<div class="col-sm-10">
				<input id="country" type="text" class="form-control" placeholder="Country">
			</div>
		</div>
		<div class="form-group">
			<label for="nickName" class="control-label">Nick</label>
			<div class="col-sm-10">
				<input id="nickName" type="text" class="form-control" placeholder="Nick Name">
			</div>
		</div>
		<div class="form-group">
			<label for="fullName" class="control-label">Full Name</label>
			<div class="col-sm-10">
				<input id="fullName" type="text" class="form-control" placeholder="Full Name">
			</div>
		</div>
		<div class="form-group">
			<label for="gender" class="control-label">Gender</label>
			<div class="col-sm-10">
				<input id="gender" type="text" class="form-control" placeholder="Gender">
			</div>
		</div>
		<div class="form-group">
			<label for="birthDate" class="control-label">Birth Date</label>
			<div class="col-sm-10">
				<input id="birthDate" type="text" class="form-control" placeholder="Birth Date">
			</div>
		</div>
	</form>
</div>
