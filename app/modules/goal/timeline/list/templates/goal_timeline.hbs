<div class="container-fluid">
	<div class="col-md-3">
		<div>
			{{smallProfileImage player}}
		</div>
		<div>
			{{bid bank.total}}
		</div>
	</div>
	<div class="col-md-9">
		<h4>
			<small>
				{{#each context.playerContexts}}
					{{formatDate clock.breachTime "%d/%m"}}
				{{/each}}
			</small>{{goal}}</h4>
		{{#if status}}<h4 class="info">{{status}}</h4>{{/if}}
		{{total}}
	</div>
</div>
