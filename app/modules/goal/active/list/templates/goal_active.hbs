<div class="container-fluid">
	<div class="row">
	<div class="col-md-3">
		<button id="update" class="btn btn-sm btn-success">Update</button><br/>
	</div>
	<div class="col-md-9">
		<h4>{{goal}} {{money configuration.bid.amount}}/<small>{{money configuration.bid.interest}}</small></h4>
		{{#if status}}<h4 class="info">{{status}}</h4>{{/if}}
		{{#each context.playerContexts}}
			Ends: {{formatDate clock.moveStart "%d/%m %H:%M:%S"}}
		{{/each}}
	</div>
	</div>
</div>
